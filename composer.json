{
    "name": "dedermus/env-manager",
    "description": "Env Manager for Laravel open-admin-core",
    "type": "library",
    "keywords": ["open-admin-core", "extension"],
    "homepage": "https://github.com/dedermus/env-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "dedermus",
            "email": "dedermus@yandex.ru"
        }
    ],
    "require": {
        "php": "~7.3|~8.0",
        "dedermus/open-admin-core": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdminCore\\EnvManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdminCore\\EnvManager\\EnvManagerServiceProvider"
            ]

        }
    }
}
